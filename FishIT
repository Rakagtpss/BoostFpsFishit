-- Tab Logic
local function SwitchTab(tabName)
    local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
    
    if tabName == "Main" then
        PageMain.Visible = true
        PageMisc.Visible = false
        
        Services.TweenService:Create(TabMainBtn, tweenInfo, {BackgroundTransparency = 0.2, TextColor3 = Color3.new(1,1,1), BackgroundColor3 = UI_THEME.Accent}):Play()
        Services.TweenService:Create(TabMiscBtn, tweenInfo, {BackgroundTransparency = 1, TextColor3 = UI_THEME.TextDim, BackgroundColor3 = Color3.fromRGB(40,40,45)}):Play()
    elseif tabName == "Misc" then
        PageMain.Visible = false
        PageMisc.Visible = true
        
        Services.TweenService:Create(TabMainBtn, tweenInfo, {BackgroundTransparency = 1, TextColor3 = UI_THEME.TextDim, BackgroundColor3 = Color3.fromRGB(40,40,45)}):Play()
        Services.TweenService:Create(TabMiscBtn, tweenInfo, {BackgroundTransparency = 0.2, TextColor3 = Color3.new(1,1,1), BackgroundColor3 = UI_THEME.Accent}):Play()
    end
end

TabMainBtn.MouseButton1Click:Connect(function() SwitchTab("Main") end)
TabMiscBtn.MouseButton1Click:Connect(function() SwitchTab("Misc") end)


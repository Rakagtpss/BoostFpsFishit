-- [FUNCTION] Logic Water Walk (Fixed)
local function SetWoW(bool)
    States.WoW = bool
    if typeof(BtnWow) == "Instance" then
        BtnWow.Text = "Water Walk: " .. (States.WoW and "ON" or "OFF")
        ToggleVisual(BtnWow, States.WoW)
    end
    
    if States.WoW then 
        if not HRP or not HRP.Parent then return end 
        
        if not BodyV then 
            BodyV = Instance.new("BodyVelocity")
            BodyV.Name = "WoW_Velocity"
            BodyV.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            BodyV.Velocity = Vector3.zero
        end
        BodyV.Parent = HRP
        
        if _G.SeraphinConnections["WoW"] then _G.SeraphinConnections["WoW"]:Disconnect() end
        
        local wowLoop = RunService.RenderStepped:Connect(function() 
            if HRP and Hum and BodyV and BodyV.Parent then
                local moveDir = Hum.MoveDirection
                local speed = Hum.WalkSpeed
                -- FIX: Keep Y velocity 0 to prevent sinking, but allow jumping if needed (limited)
                BodyV.Velocity = Vector3.new(moveDir.X * speed, 0, moveDir.Z * speed)
            end 
        end)
        _G.SeraphinConnections["WoW"] = wowLoop
    else 
        if _G.SeraphinConnections["WoW"] then 
            _G.SeraphinConnections["WoW"]:Disconnect()
            _G.SeraphinConnections["WoW"] = nil
        end 
        if BodyV then BodyV:Destroy(); BodyV = nil end 
    end 
end

-- [FUNCTION] Auto Equip (Fixed Path)
local function SetAutoEquip(bool)
    States.AutoEquip = bool
    if typeof(BtnEquip) == "Instance" then
        BtnEquip.Text = "Auto Equip: " .. (States.AutoEquip and "ON" or "OFF")
        ToggleVisual(BtnEquip, States.AutoEquip)
    end
end

-- [FUNCTION] Safe Boost (Pcall Wrapper)
local function PerformUltraClean()
    local startMem = gcinfo() 
    for i = 1, 3 do collectgarbage("collect"); task.wait(0.05) end
    
    -- Safety Check for Boost features
    local L = game:GetService("Lighting")
    L.GlobalShadows = false
    
    pcall(function()
        for _, v in pairs(Workspace:GetDescendants()) do
            if v:IsA("ParticleEmitter") or v:IsA("Trail") then v:Destroy() end
        end
    end)
    return math.floor(startMem - gcinfo())
end

-- FPS Logic
local fpsT = 0
local fpsLoop = RunService.RenderStepped:Connect(function(dt)
    fpsT = fpsT + (1/dt - fpsT) * 0.1 
    local currentFps = math.floor(fpsT)
    if Title then Title.Text = "SERAPHIN <font color=\"rgb(0,255,255)\">HELPER</font>  |  FPS: <b>" .. currentFps .. "</b>" end
end)
table.insert(_G.SeraphinConnections, fpsLoop)

BtnFps.MouseButton1Click:Connect(function()
    local caps = {30, 60, 90, 144, 240, 99999}
    _G.fpsIndex = ((_G.fpsIndex or 5) % #caps) + 1
    local v = caps[_G.fpsIndex]
    pcall(function() setfpscap(v) end) -- Protected call
    BtnFps.Text = "FPS: " .. ((v > 1000) and "Max" or tostring(v))
    ToggleVisual(BtnFps, true); wait(0.1); ToggleVisual(BtnFps, false)
end)

ToggleVisual(BtnPanel, true)
BtnPanel.MouseButton1Click:Connect(function()
    States.Panel = not States.Panel
    FloatHUD.Visible = States.Panel
    BtnPanel.Text = "Panel Info: " .. (States.Panel and "ON" or "OFF")
    ToggleVisual(BtnPanel, States.Panel)
end)

-- ====================================================
-- [5] BACKGROUND LOOPS
-- ====================================================

task.spawn(function()
    while true do
        task.wait(300) 
        if States.AutoClean then pcall(PerformUltraClean) end
    end
end)

BtnClean.MouseButton1Click:Connect(function()
    States.AutoClean = not States.AutoClean
    BtnClean.Text = "Auto Clean: " .. (States.AutoClean and "ON" or "OFF")
    ToggleVisual(BtnClean, States.AutoClean)
    if States.AutoClean then
        local freed = PerformUltraClean() 
        Notify("Ultra Clean", "Boosted: -" .. freed .. " KB (Auto 5m)")
    end
end)

-- >> LOGIC AUTO EVENT (IMPROVED SCAN)
task.spawn(function()
    while true do
        task.wait(1) 
        if States.AutoEvent then
            pcall(function()
                if not HRP or not HRP.Parent then return end
                
                -- Check Props with fallback
                local Props = Workspace:FindFirstChild("Props")
                local Target = nil
                
                if Props then
                    for _, v in pairs(Props:GetChildren()) do
                        if v:IsA("Model") or v:IsA("BasePart") then
                            Target = v; break 
                        end
                    end
                end

                if Target and Target.Parent then
                    if not IsInEvent then
                        EventStartCFrame = HRP.CFrame 
                        IsInEvent = true
                        Notify("Auto Event", "Found: " .. Target.Name)
                    end
                    
                    -- Use Pivot for better model support
                    local targetCFrame = Target:GetPivot()
                    if targetCFrame then
                        local destPos = Vector3.new(targetCFrame.Position.X, 3, targetCFrame.Position.Z)
                        if (HRP.Position - destPos).Magnitude > 10 then
                            HRP.CFrame = CFrame.new(destPos)
                        end
                    end

                    if not States.WoW then SetWoW(true) end
                else
                    if IsInEvent then
                        IsInEvent = false
                        Notify("Auto Event", "Event Ended. Returning...")
                        if EventStartCFrame and HRP then HRP.CFrame = EventStartCFrame; EventStartCFrame = nil end
                        if States.WoW then SetWoW(false) end
                    end
                end
            end)
        end
    end
end)

-- >> LOGIC AUTO EQUIP (CRITICAL FIX)
task.spawn(function()
    while true do
        task.wait(1.5)
        if States.AutoEquip then
            pcall(function()
                local char = Player.Character
                if char and not char:FindFirstChildWhichIsA("Tool") then
                    -- METODE 1: Coba Remote Spesifik (Safe)
                    local success = pcall(function()
                        local pkgs = ReplicatedStorage:FindFirstChild("Packages")
                        if pkgs then
                            local idx = pkgs:FindFirstChild("_Index")
                            if idx then
                                for _, child in pairs(idx:GetChildren()) do
                                    if child.Name:find("sleitnick_net") then -- Cari yang mirip nama ini
                                        child.net["RE/EquipToolFromHotbar"]:FireServer(1)
                                        return
                                    end
                                end
                            end
                        end
                    end)
                    
                    -- METODE 2: Fallback ke Humanoid (Jika game support)
                    if not success then
                        local backpack = Player.Backpack
                        local tool = backpack:FindFirstChildWhichIsA("Tool")
                        if tool then Char.Humanoid:EquipTool(tool) end
                    end
                end
            end)
        end
    end
end)

-- >> UPDATE STATS LOOP
task.spawn(function()
    while true do
        task.wait(0.5)
        if States.Panel then
            pcall(function()
                local ls = Player:FindFirstChild("leaderstats")
                local caughtObj = ls and ls:FindFirstChild("Caught")
                local val = caughtObj and caughtObj.Value or 0
                -- Simple number format
                LblFishVal.Text = tostring(val)

                local backpackGui = Player:FindFirstChild("PlayerGui") and Player.PlayerGui:FindFirstChild("Backpack")
                local bagText = "0/?"
                if backpackGui then
                    for _, v in pairs(backpackGui:GetDescendants()) do
                        if v:IsA("TextLabel") and v.Text:find("/") then bagText = v.Text; break end
                    end
                else 
                    bagText = tostring(#Player.Backpack:GetChildren())
                end
                LblInvVal.Text = bagText
            end)
        end
    end
end)


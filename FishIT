-- Function to create Side Buttons
local function CreateSideTab(name, text, isActive)
    local btn = Instance.new("TextButton", Sidebar)
    btn.Name = name
    btn.Size = UDim2.new(0.85, 0, 0, 32)
    btn.BackgroundColor3 = isActive and UI_THEME.Accent or Color3.fromRGB(40, 40, 45)
    btn.BackgroundTransparency = isActive and 0.2 or 1
    btn.Text = text
    btn.Font = UI_THEME.FontBody
    btn.TextSize = 11
    btn.TextColor3 = isActive and Color3.new(1,1,1) or UI_THEME.TextDim
    btn.AutoButtonColor = true
    
    local corner = Instance.new("UICorner", btn)
    corner.CornerRadius = UDim.new(0, 6)
    
    return btn
end


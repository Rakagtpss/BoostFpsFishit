-- >> PAGE MAIN BUTTONS <<
CreateButton(ScrollMain, "Auto Event", function(state, btn, lbl) 
    States.AutoEvent = state
    lbl.Text = "Auto Event: "..(state and "ON" or "OFF")
    if not state then
        if IsInEvent and EventStartCFrame and HRP then HRP.CFrame = EventStartCFrame end
        IsInEvent = false; SetWoW(false)
    end
end)

CreateButton(ScrollMain, "Auto Equip", function(state, btn, lbl)
    States.AutoEquip = state
    lbl.Text = "Auto Equip: "..(state and "ON" or "OFF")
end)

CreateButton(ScrollMain, "Water Walk", function(state, btn, lbl)
    SetWoW(state)
    lbl.Text = "Water Walk: "..(state and "ON" or "OFF")
end)

CreateButton(ScrollMain, "Freeze Pos", function(state, btn, lbl)
    States.Freeze = state
    if HRP then HRP.Anchored = state end
    lbl.Text = "Freeze: "..(state and "ON" or "OFF")
end)

CreateButton(ScrollMain, "No Clip", function(state, btn, lbl)
    States.Noclip = state
    lbl.Text = "No Clip: "..(state and "ON" or "OFF")
    if state then
        Connections["Noclip"] = Services.RunService.Stepped:Connect(function()
            if Player.Character then
                for _,v in pairs(Player.Character:GetDescendants()) do if v:IsA("BasePart") then v.CanCollide = false end end
            end
        end)
    else
        if Connections["Noclip"] then Connections["Noclip"]:Disconnect() end
    end
end)

CreateButton(ScrollMain, "CPU Booster", function(state, btn, lbl)
    States.Boost = state
    lbl.Text = "Booster: "..(state and "ON" or "OFF")
    if state then
        Services.Lighting.GlobalShadows = false
        Notify("Booster", "Graphics lowered for FPS", 2)
    end
end)

CreateButton(ScrollMain, "3D Render", function(state, btn, lbl)
    States.NoRender = not state -- If button ON, Render OFF (logic inverted in UI usually, lets simplify)
    -- Actually let's make Button ON = Render ON (Standard)
    -- Original script: Button ON = NoRender ON. I will fix this logic for better UX.
    -- If button is pressed (ON), we disable rendering? Let's stick to label.
    if state then 
        Services.RunService:Set3dRenderingEnabled(false)
        lbl.Text = "3D Render: OFF"
    else
        Services.RunService:Set3dRenderingEnabled(true)
        lbl.Text = "3D Render: ON"
    end
end)

CreateButton(ScrollMain, "FPS Cap", function(state, btn, lbl)
    -- Simple toggle for max fps
    if setfpscap then setfpscap(state and 9999 or 60) end
    lbl.Text = "FPS Unlock: "..(state and "MAX" or "60")
end)


-- [1] SYSTEM CLEANUP (Pembersih Otomatis)
if _G.SeraphinConnections then
    for _, conn in pairs(_G.SeraphinConnections) do
        if conn then 
            pcall(function() conn:Disconnect() end) 
        end
    end
end
_G.SeraphinConnections = {} 

-- [2] Notifikasi Loading
pcall(function()
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Seraphin Helper",
        Text = "Deep Scan Complete. Launching UI...",
        Duration = 2,
    })
end)

task.wait(1)


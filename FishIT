-- Background Loops
task.spawn(function() while true do task.wait(5) if States.AutoClean then pcall(function() collectgarbage("collect") end) end end end)
task.spawn(function() while true do task.wait(2) if States.AutoEquip then ForceEquipRod() end end end)

-- [AUTO EVENT LOOP - PROPS ONLY & AUTO RETURN]
local function ScanForEventProps()
    -- Hanya Check Workspace.Props sesuai request
    local props = Services.Workspace:FindFirstChild("Props")
    if props then
        for _, v in pairs(props:GetChildren()) do
            -- Filter agar tidak teleport ke dekorasi biasa
            -- Cari Model/Part yang punya ProximityPrompt ATAU nama mengandung "Event/Crate"
            if v:IsA("Model") or v:IsA("BasePart") then
                if v:FindFirstChildWhichIsA("ProximityPrompt", true) then return v end
                if v.Name:lower():find("event") or v.Name:lower():find("crate") or v.Name:lower():find("supply") then
                    return v
                end
            end
        end
    end
    return nil
end

task.spawn(function()
    while true do
        task.wait(2) -- Scan setiap 2 detik
        if States.AutoEvent then
            pcall(function()
                if not HRP or not HRP.Parent then return end
                
                local FoundEvent = ScanForEventProps()

                if FoundEvent then
                    -- EVENT KETEMU
                    if not IsInEvent then
                        EventStartCFrame = HRP.CFrame -- Simpan Posisi Awal
                        IsInEvent = true
                        Notify("AUTO EVENT", "Event in Props! Teleporting...", Color3.fromRGB(255, 255, 0))
                        SetWoW(true) -- Auto Walk On Water ON
                    end
                    
                    -- Teleport ke Event
                    local targetPos = FoundEvent:GetPivot().Position
                    local destPos = Vector3.new(targetPos.X, targetPos.Y + 4, targetPos.Z) 
                    if (HRP.Position - destPos).Magnitude > 15 then
                        HRP.CFrame = CFrame.new(destPos)
                    end
                else
                    -- EVENT HILANG (SELESAI)
                    if IsInEvent then
                        IsInEvent = false
                        Notify("AUTO EVENT", "Event Finished. Returning...", Color3.fromRGB(0, 255, 100))
                        
                        -- Teleport Balik ke Posisi Awal
                        if EventStartCFrame and HRP then 
                            HRP.CFrame = EventStartCFrame 
                            EventStartCFrame = nil 
                        end
                        SetWoW(false) -- Matikan WoW
                    end
                end
            end)
        end
    end
end)

-- [ORIGINAL BACKPACK LOGIC RESTORED]
task.spawn(function() 
    while true do 
        task.wait(1) 
        if States.Panel then 
            pcall(function() 
                local ls = Player:FindFirstChild("leaderstats")
                local c = ls and ls:FindFirstChild("Caught") and ls.Caught.Value or 0
                LblFishVal.Text = tostring(c):reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
                
                local bagText = "0/?"
                local backpackGui = Player:FindFirstChild("PlayerGui") and Player.PlayerGui:FindFirstChild("Backpack")

                if backpackGui then
                    local foundLabel = false
                    for _, v in pairs(backpackGui:GetDescendants()) do
                        if v:IsA("TextLabel") and string.find(v.Text, "/") then 
                            bagText = v.Text 
                            foundLabel = true
                            break 
                        end
                    end
                    if not foundLabel and Player:FindFirstChild("Backpack") then
                        bagText = tostring(#Player.Backpack:GetChildren())
                    end
                else 
                    if Player:FindFirstChild("Backpack") then
                         bagText = tostring(#Player.Backpack:GetChildren())
                    end
                end
                LblInvVal.Text = bagText
            end) 
        end 
    end 
end)

task.delay(1, function()
    Main.Visible = true
    FloatHUD.Visible = States.Panel 
end)

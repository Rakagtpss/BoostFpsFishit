    task.wait(0.5)
    btn.BackgroundColor3 = UI_THEME.SidebarColor
    lbl.TextColor3 = UI_THEME.TextWhite
    lbl.Font = UI_THEME.FontReg
    btn.UIStroke.Transparency = 0.7
end)


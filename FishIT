    local btn = MakeBtn(ScrollEvent, eventName)
    btn.Size = UDim2.new(0.9, 0, 0, 25)
    
    btn.MouseButton1Click:Connect(function()
        if States.SelectMode == "Priority" then
            States.EventPriority = eventName
            Notify("PRIORITY", "Set to: " .. eventName, Color3.fromRGB(0, 255, 255))
        else
            States.EventSecondary = eventName
            Notify("SECONDARY", "Set to: " .. eventName, Color3.fromRGB(255, 200, 0))
        end
    end)
end

BtnSelectMode.MouseButton1Click:Connect(function()
    if States.SelectMode == "Priority" then
        States.SelectMode = "Secondary"
        BtnSelectMode.Text = "Mode: Secondary (Select Backup)"
        BtnSelectMode.TextColor3 = Color3.fromRGB(255, 200, 0)
    else
        States.SelectMode = "Priority"
        BtnSelectMode.Text = "Mode: Priority (Select Main)"
        BtnSelectMode.TextColor3 = UI_THEME.TextMain
    end
end)


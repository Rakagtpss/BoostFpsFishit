--[[ 
 RakaHensem Utility V5.4 - Booster + Anim Scanner
]]

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local CoreGui = game:GetService("CoreGui")
local Lighting = game:GetService("Lighting")
local MaterialService = game:GetService("MaterialService")
local SoundService = game:GetService("SoundService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local Player = Players.LocalPlayer
local Char = Player.Character or Player.CharacterAdded:Wait()
local Hum = Char:WaitForChild("Humanoid")
local HRP = Char:WaitForChild("HumanoidRootPart")

-- Folder Animasi sesuai permintaanmu
local AnimFolder = ReplicatedStorage:WaitForChild("Modules"):WaitForChild("Animations")

-- State
local RenderEnabled = true
local Optimized = false
local Minimized = false
local Enabled = false
local CurrentTrack = nil
local BV, Conn

-- Hapus UI Lama
if CoreGui:FindFirstChild("RakaBoostV") then CoreGui.RakaBoostV:Destroy() end

-- UI Creation
local Gui = Instance.new("ScreenGui", CoreGui)
Gui.Name = "RakaBoostV"

local MainFrame = Instance.new("Frame", Gui)
MainFrame.Size = UDim2.new(0, 180, 0, 300)
MainFrame.Position = UDim2.new(0.5, -90, 0.3, 0)
MainFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
MainFrame.BorderSizePixel = 0
MainFrame.Active = true
MainFrame.Draggable = true
Instance.new("UICorner", MainFrame).CornerRadius = UDim.new(0, 6)

-- Header
local Header = Instance.new("Frame", MainFrame)
Header.Size = UDim2.new(1, 0, 0, 25)
Header.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Instance.new("UICorner", Header).CornerRadius = UDim.new(0, 6)

local Title = Instance.new("TextLabel", Header)
Title.Size = UDim2.new(0.7, 0, 1, 0)
Title.Position = UDim2.new(0.05, 0, 0, 0)
Title.Text = "SeraphinHelper V5.4"
Title.TextColor3 = Color3.fromRGB(0, 255, 150)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 10
Title.BackgroundTransparency = 1
Title.TextXAlignment = Enum.TextXAlignment.Left

local MinBtn = Instance.new("TextButton", Header)
MinBtn.Size = UDim2.new(0, 35, 0, 15)
MinBtn.Position = UDim2.new(0.75, 0, 0.2, 0)
MinBtn.Text = "HIDE"
MinBtn.TextColor3 = Color3.new(1, 1, 1)
MinBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
MinBtn.Font = Enum.Font.GothamBold
MinBtn.TextSize = 8
Instance.new("UICorner", MinBtn).CornerRadius = UDim.new(0, 3)

-- Scrolling Content
local ScrollFrame = Instance.new("ScrollingFrame", MainFrame)
ScrollFrame.Size = UDim2.new(1, 0, 1, -70)
ScrollFrame.Position = UDim2.new(0, 0, 0, 30)
ScrollFrame.BackgroundTransparency = 1
ScrollFrame.CanvasSize = UDim2.new(0, 0, 2, 0)
ScrollFrame.ScrollBarThickness = 2

local Layout = Instance.new("UIListLayout", ScrollFrame)
Layout.Padding = UDim.new(0, 4)
Layout.HorizontalAlignment = Enum.HorizontalAlignment.Center

local function CreateButton(text)
    local btn = Instance.new("TextButton", ScrollFrame)
    btn.Size = UDim2.new(0.9, 0, 0, 25)
    btn.Text = text
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 9
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 4)
    return btn
end

-- Fitur Booster & WOW
local WowBtn = CreateButton("WALK ON WATER: OFF")
local BoostBtn = CreateButton("ULTRA BOOSTER: OFF")
local RenderBtn = CreateButton("3D RENDERING: ON")

-- --- ANIMATION SECTION ---
local AnimLabel = Instance.new("TextLabel", ScrollFrame)
AnimLabel.Size = UDim2.new(0.9, 0, 0, 20)
AnimLabel.Text = "-- ANIMATION SCANNER --"
AnimLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
AnimLabel.BackgroundTransparency = 1
AnimLabel.Font = Enum.Font.GothamBold
AnimLabel.TextSize = 8

local AnimListFrame = Instance.new("Frame", ScrollFrame)
AnimListFrame.Size = UDim2.new(0.9, 0, 0, 80)
AnimListFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Instance.new("UICorner", AnimListFrame).CornerRadius = UDim.new(0, 4)

local AnimScroll = Instance.new("ScrollingFrame", AnimListFrame)
AnimScroll.Size = UDim2.new(1, -4, 1, -4)
AnimScroll.Position = UDim2.new(0, 2, 0, 2)
AnimScroll.BackgroundTransparency = 1
AnimScroll.CanvasSize = UDim2.new(0, 0, 5, 0)
AnimScroll.ScrollBarThickness = 2

local AnimLayout = Instance.new("UIListLayout", AnimScroll)
AnimLayout.Padding = UDim.new(0, 2)

local StopAnimBtn = CreateButton("STOP ANIMATION")
StopAnimBtn.BackgroundColor3 = Color3.fromRGB(150, 0, 0)

-- Fungsi Play Animasi
local function PlaySelectedAnim(animObject)
    if CurrentTrack then CurrentTrack:Stop() end
    
    local anim = nil
    if animObject:IsA("Animation") then
        anim = animObject
    elseif animObject:IsA("StringValue") or animObject:IsA("IntValue") then
        anim = Instance.new("Animation")
        anim.AnimationId = "rbxassetid://" .. tostring(animObject.Value)
    end
    
    if anim then
        CurrentTrack = Hum:LoadAnimation(anim)
        CurrentTrack.Looped = true
        CurrentTrack:Play()
    end
end

-- Fungsi Scan Animasi
local function ScanAnimations()
    for _, child in pairs(AnimScroll:GetChildren()) do
        if child:IsA("TextButton") then child:Destroy() end
    end
    
    for _, v in pairs(AnimFolder:GetDescendants()) do
        if v:IsA("Animation") or v.Name:lower():find("anim") then
            local btn = Instance.new("TextButton", AnimScroll)
            btn.Size = UDim2.new(1, 0, 0, 18)
            btn.Text = v.Name
            btn.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
            btn.TextColor3 = Color3.new(1, 1, 1)
            btn.Font = Enum.Font.Gotham
            btn.TextSize = 8
            
            btn.MouseButton1Click:Connect(function()
                PlaySelectedAnim(v)
            end)
        end
    end
end

StopAnimBtn.MouseButton1Click:Connect(function()
    if CurrentTrack then CurrentTrack:Stop() end
end)

-- Tombol Refresh Anim
local RefreshAnimBtn = CreateButton("REFRESH ANIMATIONS")
RefreshAnimBtn.BackgroundColor3 = Color3.fromRGB(50, 80, 120)
RefreshAnimBtn.MouseButton1Click:Connect(ScanAnimations)

-- --- FPS DISPLAY ---
local FpsDisplay = Instance.new("TextLabel", MainFrame)
FpsDisplay.Size = UDim2.new(0.9, 0, 0, 25)
FpsDisplay.Position = UDim2.new(0.05, 0, 1, -30)
FpsDisplay.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
FpsDisplay.TextColor3 = Color3.fromRGB(0, 255, 255)
FpsDisplay.Text = "FPS: --"
FpsDisplay.Font = Enum.Font.Code
FpsDisplay.TextSize = 14
Instance.new("UICorner", FpsDisplay).CornerRadius = UDim.new(0, 4)

-- --- ORIGINAL LOGIC ---

BoostBtn.MouseButton1Click:Connect(function()
    Optimized = not Optimized
    BoostBtn.Text = Optimized and "ULTRA BOOSTER: ON" or "ULTRA BOOSTER: OFF"
    BoostBtn.BackgroundColor3 = Optimized and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(35, 35, 35)
    
    Lighting.GlobalShadows = not Optimized
    for _, effect in pairs(Lighting:GetChildren()) do
        if effect:IsA("PostProcessEffect") then effect.Enabled = not Optimized end
    end
    
    settings().Rendering.QualityLevel = Optimized and 1 or 0
    for _, v in pairs(game:GetDescendants()) do
        if v:IsA("Part") or v:IsA("MeshPart") then
            if Optimized then v.Material = Enum.Material.SmoothPlastic v.CastShadow = false end
        elseif v:IsA("Decal") or v:IsA("ParticleEmitter") then v.Enabled = not Optimized end
    end
end)

RenderBtn.MouseButton1Click:Connect(function()
    RenderEnabled = not RenderEnabled
    RunService:Set3dRenderingEnabled(RenderEnabled)
    RenderBtn.Text = RenderEnabled and "3D RENDERING: ON" or "3D RENDERING: OFF"
    RenderBtn.BackgroundColor3 = RenderEnabled and Color3.fromRGB(35, 35, 35) or Color3.fromRGB(150, 0, 0)
end)

WowBtn.MouseButton1Click:Connect(function()
    Enabled = not Enabled
    WowBtn.Text = Enabled and "WOW: ON" or "WOW: OFF"
    WowBtn.BackgroundColor3 = Enabled and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(35, 35, 35)
    if Enabled then
        BV = Instance.new("BodyVelocity", HRP)
        BV.MaxForce = Vector3.new(0, 1e6, 0)
        BV.Velocity = Vector3.zero
        Conn = RunService.Heartbeat:Connect(function()
            if HRP then HRP.AssemblyLinearVelocity = Vector3.new(HRP.AssemblyLinearVelocity.X, 0, HRP.AssemblyLinearVelocity.Z) end
        end)
    else
        if Conn then Conn:Disconnect() end
        if BV then BV:Destroy() end
    end
end)

MinBtn.MouseButton1Click:Connect(function()
    Minimized = not Minimized
    MainFrame:TweenSize(Minimized and UDim2.new(0, 180, 0, 25) or UDim2.new(0, 180, 0, 300), "Out", "Quad", 0.3, true)
    ScrollFrame.Visible = not Minimized
    FpsDisplay.Visible = not Minimized
    MinBtn.Text = Minimized and "SHOW" or "HIDE"
end)

local lastT, frames = os.clock(), 0
RunService.RenderStepped:Connect(function()
    frames = frames + 1
    if os.clock() - lastT >= 1 then
        FpsDisplay.Text = "FPS: " .. frames
        frames, lastT = 0, os.clock()
    end
end)

-- Initial Scan
ScanAnimations()

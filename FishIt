--[[ 
 RakaHensem Utility V5.9 - Dynamic ID Scanner (Luck & Mutation)
]]

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local CoreGui = game:GetService("CoreGui")
local Lighting = game:GetService("Lighting")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local Player = Players.LocalPlayer
local Char = Player.Character or Player.CharacterAdded:Wait()
local HRP = Char:WaitForChild("HumanoidRootPart")

-- State
local RenderEnabled = true
local Optimized = false
local Minimized = false
local Enabled = false
local AutoTotem = false
local BV, Conn

-- Dynamic IDs Storage
local DetectedLuckID = nil
local DetectedMutationID = nil

-- Hapus UI Lama
if CoreGui:FindFirstChild("RakaBoostV") then CoreGui.RakaBoostV:Destroy() end

-- UI Creation (WIDE)
local Gui = Instance.new("ScreenGui", CoreGui)
Gui.Name = "RakaBoostV"

local MainFrame = Instance.new("Frame", Gui)
MainFrame.Size = UDim2.new(0, 360, 0, 280)
MainFrame.Position = UDim2.new(0.5, -180, 0.3, 0)
MainFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
MainFrame.BorderSizePixel = 0
MainFrame.Active = true
MainFrame.Draggable = true
Instance.new("UICorner", MainFrame).CornerRadius = UDim.new(0, 8)

-- Header
local Header = Instance.new("Frame", MainFrame)
Header.Size = UDim2.new(1, 0, 0, 30)
Header.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Instance.new("UICorner", Header).CornerRadius = UDim.new(0, 8)

local Title = Instance.new("TextLabel", Header)
Title.Size = UDim2.new(0.5, 0, 1, 0)
Title.Position = UDim2.new(0.05, 0, 0, 0)
Title.Text = "SERAPHIN HELPER V5.9"
Title.TextColor3 = Color3.fromRGB(0, 255, 150)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 12
Title.BackgroundTransparency = 1
Title.TextXAlignment = Enum.TextXAlignment.Left

local MinBtn = Instance.new("TextButton", Header)
MinBtn.Size = UDim2.new(0, 50, 0, 20)
MinBtn.Position = UDim2.new(1, -60, 0.2, 0)
MinBtn.Text = "HIDE"
MinBtn.TextColor3 = Color3.new(1, 1, 1)
MinBtn.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
MinBtn.Font = Enum.Font.GothamBold
MinBtn.TextSize = 10
Instance.new("UICorner", MinBtn).CornerRadius = UDim.new(0, 4)

-- Content Frame
local ContentFrame = Instance.new("Frame", MainFrame)
ContentFrame.Size = UDim2.new(1, -20, 1, -80)
ContentFrame.Position = UDim2.new(0, 10, 0, 40)
ContentFrame.BackgroundTransparency = 1

local Layout = Instance.new("UIGridLayout", ContentFrame)
Layout.CellSize = UDim2.new(0, 165, 0, 32)
Layout.CellPadding = UDim2.new(0, 10, 0, 8)

local function CreateButton(text, color)
    local btn = Instance.new("TextButton", ContentFrame)
    btn.Text = text
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 9
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.BackgroundColor3 = color or Color3.fromRGB(35, 35, 35)
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 4)
    return btn
end

-- ================= TOMBOL FITUR =================

local BoostBtn = CreateButton("BOOSTER: OFF")
local RenderBtn = CreateButton("3D RENDER: ON")
local WowBtn = CreateButton("WALK ON WATER: OFF")
local ScanBtn = CreateButton("ðŸ” SCAN DYNAMIC ID", Color3.fromRGB(0, 120, 255))

-- Totem Section
local SpawnMutation = CreateButton("MUTATION: ???", Color3.fromRGB(40, 40, 40))
local SpawnLuck = CreateButton("LUCK: ???", Color3.fromRGB(40, 40, 40))
local PrepareBtn = CreateButton("FORCE RE-EQUIP (SLOT 2)", Color3.fromRGB(60, 60, 60))
local AutoBtn = CreateButton("AUTO SPAWN: OFF", Color3.fromRGB(150, 0, 0))

-- ================= SCANNER LOGIC =================

local function ScanForIDs()
    DetectedLuckID = nil
    DetectedMutationID = nil
    
    -- Mencari di folder Data pemain (Fisch biasanya menyimpan item di folder khusus di Player)
    -- Kita scan Backpack sebagai fallback
    local function CheckItems(folder)
        for _, item in pairs(folder:GetChildren()) do
            local name = item.Name:lower()
            -- Jika ID ada di Name atau di dalam Attribute
            if name:find("luck") and name:find("totem") then
                DetectedLuckID = item:GetAttribute("ID") or item.Name
            elseif name:find("mutation") and name:find("totem") then
                DetectedMutationID = item:GetAttribute("ID") or item.Name
            end
        end
    end

    CheckItems(Player.Backpack)
    if Player:FindFirstChild("Data") and Player.Data:FindFirstChild("Inventory") then
        CheckItems(Player.Data.Inventory)
    end

    -- Update UI
    SpawnLuck.Text = DetectedLuckID and "LUCK: READY" or "LUCK: NOT FOUND"
    SpawnLuck.BackgroundColor3 = DetectedLuckID and Color3.fromRGB(120, 100, 0) or Color3.fromRGB(40, 40, 40)
    
    SpawnMutation.Text = DetectedMutationID and "MUTATION: READY" or "MUTATION: NOT FOUND"
    SpawnMutation.BackgroundColor3 = DetectedMutationID and Color3.fromRGB(0, 100, 70) or Color3.fromRGB(40, 40, 40)
    
    ScanBtn.Text = "SCAN FINISHED"
    task.wait(1)
    ScanBtn.Text = "ðŸ” RE-SCAN DYNAMIC ID"
end

ScanBtn.MouseButton1Click:Connect(ScanForIDs)

-- ================= LOGIC REMOTE =================

local function GetNet()
    local p = ReplicatedStorage:FindFirstChild("Packages")
    local i = p and p:FindFirstChild("_Index")
    return i and i:FindFirstChild("sleitnick_net@0.2.0") and i["sleitnick_net@0.2.0"]:FindFirstChild("net")
end

local function SmartSpawn(id)
    if not id then return end
    local Net = GetNet()
    if Net then
        Net:FindFirstChild("RE/EquipItem"):FireServer(id, "Totems")
        task.wait(0.1)
        Net:FindFirstChild("RE/ClaimNotification"):FireServer("Fish")
        task.wait(0.1)
        Net:FindFirstChild("RE/EquipToolFromHotbar"):FireServer(2)
        task.wait(0.3)
        Net:FindFirstChild("RE/SpawnTotem"):FireServer(id)
    end
end

SpawnMutation.MouseButton1Click:Connect(function() SmartSpawn(DetectedMutationID) end)
SpawnLuck.MouseButton1Click:Connect(function() SmartSpawn(DetectedLuckID) end)

-- AUTO SPAWN LOOP
task.spawn(function()
    while true do
        if AutoTotem then
            if DetectedMutationID then SmartSpawn(DetectedMutationID) task.wait(1) end
            if DetectedLuckID then SmartSpawn(DetectedLuckID) end
            task.wait(3600)
        end
        task.wait(1)
    end
end)

AutoBtn.MouseButton1Click:Connect(function()
    AutoTotem = not AutoTotem
    AutoBtn.Text = AutoTotem and "AUTO SPAWN: ON" or "AUTO SPAWN: OFF"
    AutoBtn.BackgroundColor3 = AutoTotem and Color3.fromRGB(0, 150, 0) or Color3.fromRGB(150, 0, 0)
end)

-- ================= UTILITY LOGIC =================

BoostBtn.MouseButton1Click:Connect(function()
    Optimized = not Optimized
    BoostBtn.Text = Optimized and "BOOSTER: ACTIVE" or "BOOSTER: OFF"
    settings().Rendering.QualityLevel = Optimized and 1 or 0
    Lighting.GlobalShadows = not Optimized
end)

RenderBtn.MouseButton1Click:Connect(function()
    RenderEnabled = not RenderEnabled
    RunService:Set3dRenderingEnabled(RenderEnabled)
    RenderBtn.Text = RenderEnabled and "3D RENDER: ON" or "3D RENDER: OFF"
end)

WowBtn.MouseButton1Click:Connect(function()
    Enabled = not Enabled
    WowBtn.Text = Enabled and "WOW: ON" or "WOW: OFF"
    if Enabled then
        BV = Instance.new("BodyVelocity", HRP)
        BV.MaxForce = Vector3.new(0, 1e6, 0)
        BV.Velocity = Vector3.zero
        Conn = RunService.Heartbeat:Connect(function()
            if HRP then HRP.AssemblyLinearVelocity = Vector3.new(HRP.AssemblyLinearVelocity.X, 0, HRP.AssemblyLinearVelocity.Z) end
        end)
    else
        if Conn then Conn:Disconnect() end
        if BV then BV:Destroy() end
    end
end)

-- Status Bar
local FpsDisplay = Instance.new("TextLabel", MainFrame)
FpsDisplay.Size = UDim2.new(1, -20, 0, 30)
FpsDisplay.Position = UDim2.new(0, 10, 1, -40)
FpsDisplay.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
FpsDisplay.TextColor3 = Color3.fromRGB(0, 255, 255)
FpsDisplay.Text = "FPS: -- | READY"
FpsDisplay.Font = Enum.Font.Code
FpsDisplay.TextSize = 11
Instance.new("UICorner", FpsDisplay).CornerRadius = UDim.new(0, 4)

MinBtn.MouseButton1Click:Connect(function()
    Minimized = not Minimized
    MainFrame:TweenSize(Minimized and UDim2.new(0, 360, 0, 30) or UDim2.new(0, 360, 0, 280), "Out", "Quad", 0.3, true)
    ContentFrame.Visible = not Minimized
    FpsDisplay.Visible = not Minimized
end)

local lastT, frames = os.clock(), 0
RunService.RenderStepped:Connect(function()
    frames = frames + 1
    if os.clock() - lastT >= 1 then
        FpsDisplay.Text = "FPS: " .. frames .. " | AUTO: " .. (AutoTotem and "ON" or "OFF")
        frames, lastT = 0, os.clock()
    end
end)

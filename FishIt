--[[ 
 RakaHensem Utility V5 - Modular Optimized (Auto Fish Integrated)
]]

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local CoreGui = game:GetService("CoreGui")
local Lighting = game:GetService("Lighting")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local Player = Players.LocalPlayer
local Char = Player.Character or Player.CharacterAdded:Wait()
local HRP = Char:WaitForChild("HumanoidRootPart")

-- State
local RenderEnabled = true
local Optimized = false
local Minimized = false
local Enabled = false
local BV, Conn

-- Fishing Config & State
getgenv().fishingStart = false
local delayReel = 0.56   -- Default DelayTime
local delayBait = 1.15   -- Default DelayCharge
local delayReset = 0.15 
local args = { -1.115296483039856, 0, 1763651451.636425 }

-- Remote References (Adjust paths if necessary for your specific game version)
local NetPath = ReplicatedStorage:WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_net@0.2.0"):WaitForChild("net")
local ChargeRod = NetPath:WaitForChild("RF/ChargeFishingRod") -- Example path
local RequestGame = NetPath:WaitForChild("RF/RequestFishingMinigameStarted")
local CompleteGame = NetPath:WaitForChild("RE/CompleteFishingMinigame")
local CancelInput = NetPath:WaitForChild("RF/CancelFishingInput")

-- Hapus UI Lama
if CoreGui:FindFirstChild("RakaBoostV") then CoreGui.RakaBoostV:Destroy() end

-- UI Creation
local Gui = Instance.new("ScreenGui", CoreGui)
Gui.Name = "RakaBoostV"

local MainFrame = Instance.new("Frame", Gui)
MainFrame.Size = UDim2.new(0, 160, 0, 320) -- Increased height for fishing controls
MainFrame.Position = UDim2.new(0.5, -80, 0.3, 0)
MainFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
MainFrame.BorderSizePixel = 0
MainFrame.Active = true
MainFrame.Draggable = true
Instance.new("UICorner", MainFrame).CornerRadius = UDim.new(0, 6)

-- Header
local Header = Instance.new("Frame", MainFrame)
Header.Size = UDim2.new(1, 0, 0, 25)
Header.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Instance.new("UICorner", Header).CornerRadius = UDim.new(0, 6)

local Title = Instance.new("TextLabel", Header)
Title.Size = UDim2.new(0.7, 0, 1, 0)
Title.Position = UDim2.new(0.05, 0, 0, 0)
Title.Text = "SeraphinHelper V5"
Title.TextColor3 = Color3.fromRGB(0, 255, 150)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 10
Title.BackgroundTransparency = 1
Title.TextXAlignment = Enum.TextXAlignment.Left

local MinBtn = Instance.new("TextButton", Header)
MinBtn.Size = UDim2.new(0, 35, 0, 15)
MinBtn.Position = UDim2.new(0.75, 0, 0.2, 0)
MinBtn.Text = "HIDE"
MinBtn.TextColor3 = Color3.new(1, 1, 1)
MinBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
MinBtn.Font = Enum.Font.GothamBold
MinBtn.TextSize = 8
Instance.new("UICorner", MinBtn).CornerRadius = UDim.new(0, 3)

-- Scrolling Content
local ScrollFrame = Instance.new("ScrollingFrame", MainFrame)
ScrollFrame.Size = UDim2.new(1, 0, 1, -65)
ScrollFrame.Position = UDim2.new(0, 0, 0, 30)
ScrollFrame.BackgroundTransparency = 1
ScrollFrame.CanvasSize = UDim2.new(0, 0, 1.5, 0)
ScrollFrame.ScrollBarThickness = 2

local Layout = Instance.new("UIListLayout", ScrollFrame)
Layout.Padding = UDim.new(0, 4)
Layout.HorizontalAlignment = Enum.HorizontalAlignment.Center

local function CreateButton(text)
    local btn = Instance.new("TextButton", ScrollFrame)
    btn.Size = UDim2.new(0.9, 0, 0, 25)
    btn.Text = text
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 9
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 4)
    return btn
end

local function CreateInput(placeholder)
    local input = Instance.new("TextBox", ScrollFrame)
    input.Size = UDim2.new(0.9, 0, 0, 25)
    input.PlaceholderText = placeholder
    input.Text = ""
    input.Font = Enum.Font.Gotham
    input.TextSize = 9
    input.TextColor3 = Color3.new(1, 1, 1)
    input.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    Instance.new("UICorner", input).CornerRadius = UDim.new(0, 4)
    return input
end

--- --- UI ELEMENTS --- ---
local BoostBtn = CreateButton("BOOSTER (LOW GRAPHIC)")
local RenderBtn = CreateButton("3D RENDERING: ON")
local WowBtn = CreateButton("WALK ON WATER: OFF")

-- Fishing Section
local FishLabel = Instance.new("TextLabel", ScrollFrame)
FishLabel.Size = UDim2.new(0.9, 0, 0, 20)
FishLabel.Text = "--- AUTO FISHING ---"
FishLabel.TextColor3 = Color3.fromRGB(0, 255, 150)
FishLabel.BackgroundTransparency = 1
FishLabel.Font = Enum.Font.GothamBold
FishLabel.TextSize = 9

local ReelInput = CreateInput("Delay Reel (Default 0.56)")
local BaitInput = CreateInput("Delay Bait (Default 1.15)")
local FishToggle = CreateButton("AUTO FISH: OFF")

-- FPS Display
local FpsDisplay = Instance.new("TextLabel", MainFrame)
FpsDisplay.Size = UDim2.new(0.9, 0, 0, 25)
FpsDisplay.Position = UDim2.new(0.05, 0, 1, -30)
FpsDisplay.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
FpsDisplay.TextColor3 = Color3.fromRGB(0, 255, 255)
FpsDisplay.Text = "FPS: --"
FpsDisplay.Font = Enum.Font.Code
FpsDisplay.TextSize = 14
Instance.new("UICorner", FpsDisplay).CornerRadius = UDim.new(0, 4)

--- --- FISHING LOGIC --- ---
local function startFishingSuperInstantLoop()
    while getgenv().fishingStart do
        pcall(function() 
            ChargeRod:InvokeServer()
            task.wait(0.055)
            RequestGame:InvokeServer(unpack(args))
            task.wait(delayBait) 
            CompleteGame:FireServer()
            task.wait(delayReset) 
            CancelInput:InvokeServer()
            task.wait(delayReel)
        end)
    end
end

FishToggle.MouseButton1Click:Connect(function()
    getgenv().fishingStart = not getgenv().fishingStart
    
    -- Update delays from Inputs
    delayReel = tonumber(ReelInput.Text) or 0.56
    delayBait = tonumber(BaitInput.Text) or 1.15
    
    FishToggle.Text = getgenv().fishingStart and "AUTO FISH: ON" or "AUTO FISH: OFF"
    FishToggle.BackgroundColor3 = getgenv().fishingStart and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(35, 35, 35)
    
    if getgenv().fishingStart then
        task.spawn(startFishingSuperInstantLoop)
    end
end)

--- --- UTILITY LOGIC --- ---
BoostBtn.MouseButton1Click:Connect(function()
    Optimized = not Optimized
    BoostBtn.Text = Optimized and "BOOSTER: ACTIVE" or "BOOSTER: OFF"
    BoostBtn.BackgroundColor3 = Optimized and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(35, 35, 35)
    settings().Rendering.QualityLevel = Optimized and 1 or 0
    Lighting.GlobalShadows = not Optimized
    for _, v in pairs(game:GetDescendants()) do
        if v:IsA("Part") or v:IsA("MeshPart") or v:IsA("UnionOperation") then
            if Optimized then v.Material = Enum.Material.SmoothPlastic v.CastShadow = false end
        elseif v:IsA("Decal") or v:IsA("Texture") or v:IsA("ParticleEmitter") then
            v.Enabled = not Optimized
        end
    end
end)

RenderBtn.MouseButton1Click:Connect(function()
    RenderEnabled = not RenderEnabled
    RunService:Set3dRenderingEnabled(RenderEnabled)
    RenderBtn.Text = RenderEnabled and "3D RENDERING: ON" or "3D RENDERING: OFF"
    RenderBtn.BackgroundColor3 = RenderEnabled and Color3.fromRGB(35, 35, 35) or Color3.fromRGB(150, 0, 0)
end)

WowBtn.MouseButton1Click:Connect(function()
    Enabled = not Enabled
    WowBtn.Text = Enabled and "WOW: ON" or "WOW: OFF"
    WowBtn.BackgroundColor3 = Enabled and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(35, 35, 35)
    if Enabled then
        BV = Instance.new("BodyVelocity", HRP)
        BV.MaxForce = Vector3.new(0, 1e6, 0)
        BV.Velocity = Vector3.zero
        Conn = RunService.Heartbeat:Connect(function()
            if HRP then HRP.AssemblyLinearVelocity = Vector3.new(HRP.AssemblyLinearVelocity.X, 0, HRP.AssemblyLinearVelocity.Z) end
        end)
    else
        if Conn then Conn:Disconnect() end
        if BV then BV:Destroy() end
    end
end)

-- Minimize
MinBtn.MouseButton1Click:Connect(function()
    Minimized = not Minimized
    MainFrame:TweenSize(Minimized and UDim2.new(0, 160, 0, 25) or UDim2.new(0, 160, 0, 320), "Out", "Quad", 0.3, true)
    ScrollFrame.Visible = not Minimized
    FpsDisplay.Visible = not Minimized
    MinBtn.Text = Minimized and "SHOW" or "HIDE"
end)

-- FPS Counter
local lastT, frames = os.clock(), 0
RunService.RenderStepped:Connect(function()
    frames = frames + 1
    if os.clock() - lastT >= 1 then
        FpsDisplay.Text = "FPS: " .. frames
        frames, lastT = 0, os.clock()
    end
end)
